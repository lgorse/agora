<%= form_for(:session, :url => sessions_path, :html => {:class => "login_form"}) do |session| %>
<h4>Log in with your Venture Studio E-mail</h4>
<%= session.label :email %>
<%= session.text_field :email %>
<%= session.submit "Sign in" %>
<% end %>
<% flash.each do |key, value| %>
<%= value %>
<% end %>